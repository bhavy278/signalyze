{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "rootDir": "./src",
    "typeRoots": ["./node_modules/@types", "./src/@types"],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts","src/**/*.sql", "src/commons/types/**/*.d.ts", "src/**/*"]


}
